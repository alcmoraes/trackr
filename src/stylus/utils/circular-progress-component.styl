/**
 * Circular Progress Component Spinner
 */
.circular-progress-wrapper
    position absolute
    display none
    width 100%
    height 100%
    top 0
    right 0
    bottom 0
    left 0
    background rgba(250,250,250,0.8)
    z-index 9999
    > div
        margin 0!important
        top 50%
        left 50%
        width 60px!important
        height 60px!important
        margin-top: -30px!important
        margin-left: -30px!important
